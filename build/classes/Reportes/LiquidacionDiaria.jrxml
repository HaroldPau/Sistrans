	<queryString>
		<![CDATA[select v.padron,(p.apellido_pa+' '+p.apellido_ma +', ' + p.nombre) nombres,sum(dl.cantidad)pasajeros, l.vuelta , sum (dl.cantidad*precio)recaudado  from det_liquidacion dl 
